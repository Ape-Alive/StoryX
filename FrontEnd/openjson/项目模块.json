  "paths": {
    "/api/projects": {
      "get": {
        "summary": "获取当前用户的所有项目",
        "deprecated": false,
        "description": "获取当前登录用户的所有项目列表，支持分页和状态筛选",
        "tags": [
          "Projects"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "页码",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "每页数量",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 10
            }
          },
          {
            "name": "status",
            "in": "query",
            "description": "项目状态筛选",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "pending",
                "processing",
                "reviewing",
                "generating",
                "rendering",
                "completed",
                "failed"
              ]
            }
          },
          {
            "name": "Authorization",
            "in": "header",
            "description": "",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjNkMjAyOTcwLWVmMWUtNDE3ZC1hNzQxLTA5MmJmNjJhNDdhMCIsImlhdCI6MTc2NTc2NzM3MiwiZXhwIjoxNzY2MzcyMTcyfQ.lL4VWjIYSSLeQt6kMcqUHjrudya_PBDkPP0AfE2_QSA",
            "schema": {
              "type": "string",
              "default": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjNkMjAyOTcwLWVmMWUtNDE3ZC1hNzQxLTA5MmJmNjJhNDdhMCIsImlhdCI6MTc2NTc2NzM3MiwiZXhwIjoxNzY2MzcyMTcyfQ.lL4VWjIYSSLeQt6kMcqUHjrudya_PBDkPP0AfE2_QSA"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "成功返回项目列表",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Success"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "data": {
                          "type": "object",
                          "properties": {
                            "projects": {
                              "type": "array",
                              "items": {
                                "$ref": "#/components/schemas/Project"
                              }
                            },
                            "pagination": {
                              "type": "object",
                              "properties": {
                                "page": {
                                  "type": "integer"
                                },
                                "limit": {
                                  "type": "integer"
                                },
                                "total": {
                                  "type": "integer"
                                },
                                "pages": {
                                  "type": "integer"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            },
            "headers": {}
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      },
      "post": {
        "summary": "创建新项目",
        "deprecated": false,
        "description": "创建一个新的动画项目，支持两种模式：自定义模式（custom）需要配置AI模型和存储位置；普通模式（normal）会从全局配置读取AI模型配置",
        "tags": [
          "Projects"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjNkMjAyOTcwLWVmMWUtNDE3ZC1hNzQxLTA5MmJmNjJhNDdhMCIsImlhdCI6MTc2NTc2NzM3MiwiZXhwIjoxNzY2MzcyMTcyfQ.lL4VWjIYSSLeQt6kMcqUHjrudya_PBDkPP0AfE2_QSA",
            "schema": {
              "type": "string",
              "default": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjNkMjAyOTcwLWVmMWUtNDE3ZC1hNzQxLTA5MmJmNjJhNDdhMCIsImlhdCI6MTc2NTc2NzM3MiwiZXhwIjoxNzY2MzcyMTcyfQ.lL4VWjIYSSLeQt6kMcqUHjrudya_PBDkPP0AfE2_QSA"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "title",
                  "storageLocation"
                ],
                "properties": {
                  "title": {
                    "type": "string",
                    "example": "我的第一个动画项目"
                  },
                  "description": {
                    "type": "string",
                    "example": "项目描述"
                  },
                  "sourceText": {
                    "type": "string",
                    "example": "小说文本内容"
                  },
                  "mode": {
                    "type": "string",
                    "enum": [
                      "custom",
                      "normal"
                    ],
                    "default": "custom",
                    "description": "创建模式，custom为自定义模式（需要提供所有配置），normal为普通模式（从全局配置读取）",
                    "example": "normal"
                  },
                  "configLLM": {
                    "type": "string",
                    "format": "uuid",
                    "description": "LLM模型ID（custom模式必填，normal模式可选，会从全局配置读取）",
                    "example": "uuid-of-llm-model"
                  },
                  "configLLMKey": {
                    "type": "string",
                    "description": "LLM API密钥（custom模式：如果配置了configLLM则必填；normal模式：从全局配置读取）",
                    "example": "sk-xxxxxxxxxxxxx"
                  },
                  "configVideoAI": {
                    "type": "string",
                    "format": "uuid",
                    "description": "视频AI模型ID（custom模式必填，normal模式可选，会从全局配置读取）",
                    "example": "uuid-of-video-model"
                  },
                  "configVideoAIKey": {
                    "type": "string",
                    "description": "视频AI API密钥（custom模式：如果配置了configVideoAI则必填；normal模式：从全局配置读取）",
                    "example": "video-api-key-xxxxx"
                  },
                  "configTTS": {
                    "type": "string",
                    "format": "uuid",
                    "description": "TTS模型ID（custom模式必填，normal模式可选，会从全局配置读取）",
                    "example": "uuid-of-tts-model"
                  },
                  "configTTSKey": {
                    "type": "string",
                    "description": "TTS API密钥（custom模式：如果配置了configTTS则必填；normal模式：从全局配置读取）",
                    "example": "tts-api-key-xxxxx"
                  },
                  "configImageGen": {
                    "type": "string",
                    "format": "uuid",
                    "description": "图片生成模型ID（custom模式必填，normal模式可选，会从全局配置读取）",
                    "example": "uuid-of-image-model"
                  },
                  "configImageGenKey": {
                    "type": "string",
                    "description": "图片生成API密钥（custom模式：如果配置了configImageGen则必填；normal模式：从全局配置读取）",
                    "example": "sd-api-key-xxxxx"
                  },
                  "storageLocation": {
                    "type": "string",
                    "enum": [
                      "local",
                      "remote"
                    ],
                    "description": "存储位置",
                    "example": "local"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "项目创建成功",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Success"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/Project"
                        }
                      }
                    }
                  ]
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "请求参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "headers": {}
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
