{
  "openapi": "3.0.1",
  "info": {
    "title": "默认模块",
    "description": "StoryX Backend API - AI-powered animation video generation platform",
    "version": "1.0.0",
    "contact": {
      "name": "StoryX Team"
    },
    "license": {
      "name": "ISC"
    }
